{
  "cli": {
    "version": ">= 8.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "autoIncrement": true,
      "env": { 
        "EXPO_PUBLIC_API_BASE_URL": "https://stage.api.culero.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
      
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://api.culero.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
